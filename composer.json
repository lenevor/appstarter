{
	"name": "lenevor/appstarter",
	"type": "project",
	"description": "The Lenevor Framework.",
	"keywords": ["syscodes","lenevor"],
	"license": "BSD-3-Clause",
	"homepage": "https://lenevor.com",
	"support": {
		"wiki": "https://github.com/lenevor/lenevor/wiki",
		"source": "https://github.com/lenevor"
	},
	"authors": [
		{
			"name": "Alexander Campo",
			"email": "jalexcam@gmail.com"
		}
	],
	"require": {
		"php": "^8.2",
		"lenevor/syscodes": "^0.8.14",
		"nunomaduro/termwind": "^2.0",
		"psr/container": "^1.1.1|^2.0.2",
		"psr/log": "^1.0|^2.0|^3.0",
		"symfony/console": "^7.2.0",
		"symfony/error-handler": "^7.2.0",
		"symfony/http-foundation": "^7.2.0",
		"symfony/http-kernel": "^7.2.0",
		"symfony/process": "^7.2.0",
		"symfony/routing": "^7.2.0",
		"symfony/var-dumper": "^7.2.0"
	},
	"scripts": {
		"setup": [
			"composer install",
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
			"@php prime key:generate"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],	
		"post-create-project-cmd": [
			"@php prime key:generate --ansi"
		]
	},
	"extra": {
		"lenevor": {
			"dont-discover": []
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}